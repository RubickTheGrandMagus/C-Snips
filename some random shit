// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
    int arr[7] = {2,3,4,5,6,7,8};
    int dvd = 2; int total = 7; int indx = total/dvd;
    for(int i=0; i<total; i++){
        if(i<indx){
           cout << i+arr[i];
           cout << "\n";
        }else{
            indx = (indx/dvd) + i;
            i--;
        }
    }

    return 0;
}
